<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

<bean class="com.nt.beans.SampleBean"><!-- default bean id :: com.nt.beans.SampleBean (or) com.nt.beans.SampleBean#0 -->
	<property name="name" value="Praveen"/>
	<property name="age" value="23"></property>
</bean>
<bean class="com.nt.beans.SampleBean"><!-- default bean id :: com.nt.beans.SampleBean#1 -->
	<property name="name" value="Sujatha"/>
	<property name="age" value="49"/>
</bean>
<bean class="com.nt.beans.SampleBean"><!-- default bean id :: com.nt.beans.SampleBean#2 -->
	<property name="name" value="Prasad"/>
	<property name="age" value="58"></property>
</bean>

<bean class="java.util.Date"/><!-- default bean id :: java.util.Date (or) java.util.Date#0 -->
<bean class="java.util.Date"><!-- default bean id :: java.util.Date#1 -->
	<property name="date" value="10"/>
</bean>

</beans>
